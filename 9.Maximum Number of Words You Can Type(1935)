/*T.C=O(n+m), S.C=O(26)*/
class Solution {
public:
    int canBeTypedWords(string text, string brokenLetters) {
        bool mp[26];
        for(char &ch:brokenLetters){
            mp[ch-'a']=true;
        }
        int result=0;
        bool canType=true;
        for(char &ch:text){
            if(ch==' '){
                //I have completed one word
                if(canType) result++;
                canType=true; 
            }
            else if(mp[ch-'a']==true){
                //broken characters
                canType=false;
            }
        }
        if(canType) result++;
    
    
        return result;
    }
};
