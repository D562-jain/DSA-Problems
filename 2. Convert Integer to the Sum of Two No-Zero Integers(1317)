class Solution {
public:
    bool hasNoZero(int num){
        while(num>0){
            if(num%10==0) return false;
            num/=10;
        }
        return true;
    }
    vector<int> getNoZeroIntegers(int n) {
       for(int num=1; num<n; num++){
            int nextNum=n-num;
            if(hasNoZero(num) && hasNoZero(nextNum)) return {num, nextNum};
        }
        return {};
        
    }
};
